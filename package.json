{
  "name": "chartex",
  "version": "0.3.0",
  "description": "A data visualization library for the terminal using ASCII characters.",
  "type": "module",
  "homepage": "https://metalbolicx.github.io/chartex/#/",
  "exports": {
    "import": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "require": {
      "types": "./dist/index.d.cts",
      "default": "./dist/index.cjs"
    }
  },
  "engines": {
    "node": ">=22.0.0"
  },
  "module": "./dist/index.js",
  "main": "dist/index.cjs",
  "types": "dist/index.d.ts",
  "scripts": {
    "docs": "docsify serve docs",
    "start": "echo \"Hello world\"",
    "build": "tsup src/index.ts --format esm,cjs --dts --minify"
  },
  "author": {
    "name": "José Martínez Santana"
  },
  "keywords": [
    "terminal",
    "data-visualization",
    "ascii",
    "charting",
    "cli",
    "javascript",
    "shell"
  ],
  "repository": {
    "type": "github",
    "url": "https://github.com/MetalbolicX/chartex.git"
  },
  "bugs": {
    "url": "https://github.com/MetalbolicX/chartex/issues"
  },
  "files": [
    "dist"
  ],
  "license": "MIT",
  "devDependencies": {
    "tsup": "^8.5.0"
  }
}
